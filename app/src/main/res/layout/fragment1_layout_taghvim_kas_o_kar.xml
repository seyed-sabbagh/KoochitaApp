<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/color_XD_Dark"
    android:layout_height="match_parent">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:background="@color/color_XD_lite"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent">


    </LinearLayout>

    <ImageView
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="5dp"
        android:src="@drawable/img_toolbar"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout2" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/iransans"
        android:text="@string/taghvim_kasbO_Kar"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="@+id/linearLayout2" />


    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_margin="10dp"
        android:layout_marginTop="50dp"
        android:background="@color/color_XD_lite"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="10dp">


    </RelativeLayout>


    <TextView
        android:id="@+id/textView52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="@font/iransans"
        android:text="@string/shanbe"
        android:textColor="@color/yellow_XD"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/relativeLayout5"
        app:layout_constraintTop_toTopOf="@+id/relativeLayout5" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/iransans"
        android:text="@string/tedad_mosaferan"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/relativeLayout5"
        app:layout_constraintTop_toBottomOf="@+id/textView52" />


    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/iransans"
        android:text="@string/_16"
        android:textColor="@color/yellow_XD"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView53"
        app:layout_constraintEnd_toStartOf="@+id/textView53"
        app:layout_constraintTop_toTopOf="@+id/textView53" />


    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/iransans"
        android:text="@string/zarfiyat_khali"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView54"
        app:layout_constraintEnd_toStartOf="@+id/textView54"
        app:layout_constraintTop_toTopOf="@+id/textView54" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/iransans"
        android:text="@string/_25"
        android:textColor="@color/yellow_XD"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView55"
        app:layout_constraintEnd_toStartOf="@+id/textView55"
        app:layout_constraintTop_toTopOf="@+id/textView55" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/iransans"
        android:text="@string/darAmad"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView56"
        app:layout_constraintEnd_toStartOf="@+id/textView56"
        app:layout_constraintTop_toTopOf="@+id/textView56" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/iransans"
        android:text="250.000"
        android:textColor="@color/yellow_XD"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView57"
        app:layout_constraintEnd_toStartOf="@+id/textView57"
        app:layout_constraintTop_toTopOf="@+id/textView57" />

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="@font/iransans"
        android:text="@string/otagh_por"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/relativeLayout5"
        app:layout_constraintTop_toBottomOf="@+id/textView53" />

    <TextView

        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:fontFamily="@font/iransans"
        android:text="اتاق پر , اتاق پر , اتاق پر"
        android:textColor="@color/yellow_XD"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView58"
        app:layout_constraintEnd_toStartOf="@+id/textView58"
        app:layout_constraintTop_toTopOf="@+id/textView58" />

    <TextView

        android:id="@+id/textView60"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="@font/iransans"
        android:text="@string/otagh_khali"
        android:textColor="@color/white"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/relativeLayout5"
        app:layout_constraintTop_toBottomOf="@+id/textView58" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:layout_marginEnd="5dp"
        android:fontFamily="@font/iransans"
        android:text="اتاق پر , اتاق پر , اتاق پر"
        android:textColor="@color/yellow_XD"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView60"
        app:layout_constraintEnd_toStartOf="@+id/textView60"
        app:layout_constraintTop_toTopOf="@+id/textView60" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/ic_down"
        app:layout_constraintBottom_toBottomOf="@+id/relativeLayout5"
        app:layout_constraintStart_toStartOf="@+id/relativeLayout5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/iransans"
        android:text="@string/moshahede_hame"
        android:textColor="@android:color/holo_purple"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/relativeLayout5"
        app:layout_constraintStart_toEndOf="@+id/imageView3" />


    <TextView
        android:id="@+id/textView70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:fontFamily="@font/iransans"
        android:text="@string/mosafer"
        android:textColor="@color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="@+id/viesws"
        app:layout_constraintEnd_toEndOf="@+id/relativeLayout5"
        app:layout_constraintTop_toTopOf="@+id/viesws" />

    <androidx.cardview.widget.CardView
        android:id="@+id/viesws"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:elevation="12dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/viesw"
        app:layout_constraintEnd_toStartOf="@+id/textView70"
        app:layout_constraintTop_toBottomOf="@+id/textView60"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/circleImagess"
            android:layout_width="30dp"
            android:layout_height="30dp"

            android:scaleType="centerCrop"
            android:src="@drawable/keshti_banner" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.8"
            android:src="#EFEFEF" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView

        android:id="@+id/cardView8"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="15dp"
        android:elevation="12dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/viesws"
        app:layout_constraintEnd_toStartOf="@+id/viesws"
        app:layout_constraintTop_toTopOf="@+id/viesws">

        <ImageView
            android:id="@+id/circleImagess"
            android:layout_width="30dp"
            android:layout_height="30dp"

            android:scaleType="centerCrop"
            android:src="@drawable/keshti_banner" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.8"
            android:src="#EFEFEF" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView

        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="15dp"
        android:elevation="12dp"
        app:cardCornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/cardView8"
        app:layout_constraintEnd_toStartOf="@+id/cardView8"
        app:layout_constraintTop_toTopOf="@+id/cardView8">

        <ImageView
            android:id="@+id/circleImagess"
            android:layout_width="30dp"
            android:layout_height="30dp"

            android:scaleType="centerCrop"
            android:src="@drawable/keshti_banner" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.8"
            android:src="#EFEFEF" />

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>